{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowWriteToLexLogGroups",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "logs:PutRetentionPolicy"
            ],
            "Resource": "arn:aws:logs:us-east-1:654654520205:log-group:/aws/lex/*:log-stream:*"
        }
    ]
}