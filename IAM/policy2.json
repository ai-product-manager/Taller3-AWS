{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BedrockAgentInvokePolicy",
            "Effect": "Allow",
            "Action": [
                "bedrock:InvokeAgent"
            ],
            "Resource": [
                "arn:aws:bedrock:*:654654520205:agent-alias/BPAKKNWPGF/*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:ResourceAccount": "654654520205"
                }
            }
        }
    ]
}